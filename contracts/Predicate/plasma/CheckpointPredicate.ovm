from lib import LessThan
from lib import IncludedAt

@library
@quantifier("proof.block${b}.range${token},RANGE,${range}")
def SU(so, token, range, b) :=
  IncludedAt(proof, token, range, b, $commitmentContract).any()

def checkpoint(su, proof) :=
  VerifyInclusion(su.3, su.0, su.1, proof, su.2)
  and LessThan(su.2).all(b -> 
    SU(su.0, su.1, b).all(old_su -> old_su())
  )
